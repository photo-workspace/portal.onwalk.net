{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=24 <25"
  },
  "scripts": {
    "dev": "bash scripts/Dev-MCP-Server.sh && next dev --turbo",
    "dev:raw": "next dev --turbo",
    "prebuild": "bash scripts/prebuild.sh",
    "build": "next build",
    "build:app": "APP_BUILD=true next build",
    "build:static": "npm run prebuild && next build",
    "prod": "bash scripts/prod.sh",
    "start": "node ./scripts/start.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "meta-index:to-md": "tsx scripts/meta-index-to-md.ts",
    "mcp:meta-index": "tsx scripts/mcp-server.ts",
    "preview": "next build && next start",
    "mcp:server": "tsx scripts/mcp-server.ts",
    "mcp:server:meta-index": "tsx scripts/mcp-server.ts",
    "test": "vitest run --config tests/unit/vitest.config.ts",
    "test:unit": "vitest run --config tests/unit/vitest.config.ts",
    "test:e2e": "playwright test --config tests/e2e/playwright.config.ts"
  },
